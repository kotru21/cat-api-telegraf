{
  "name": "tg-bot-telegraph",
  "version": "5.0.0",
  "description": "Telegraf.js cats bot with web panel",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "bun run scripts/init-db.js && bun src/index.js",
    "smoke": "bun scripts/smoke.js",
    "prisma:check": "bun scripts/check-prisma.js",
    "test": "bun test --preload ./tests/setup/setupEnv.js",
    "prisma:generate": "bunx prisma generate",
    "prisma:generate:dev": "bunx prisma generate --schema=prisma/schema.sqlite.prisma",
    "prisma:migrate:dev": "bunx prisma migrate dev --name init",
    "prisma:migrate:dev:sqlite": "bunx prisma migrate dev --schema=prisma/schema.sqlite.prisma --name init",
    "prisma:migrate:deploy": "bunx prisma migrate deploy",
    "prisma:migrate:reset": "bunx prisma migrate reset --force",
    "prisma:studio": "bunx prisma studio"
  },
  "author": "kotru21",
  "license": "ISC",
  "engines": {
    "node": ">=18.18.0",
    "bun": ">=1.0.0"
  },
  "dependencies": {
    "@libsql/client": "^0.15.15",
    "@prisma/adapter-libsql": "^7.0.0",
    "@prisma/client": "^7.0.0",
    "awilix": "^12.0.5",
    "connect-redis": "^9.0.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-rate-limit": "^8.2.1",
    "express-session": "^1.18.2",
    "helmet": "^8.1.0",
    "pino": "^10.1.0",
    "pino-http": "^11.0.0",
    "pino-pretty": "^13.1.2",
    "redis": "^5.10.0",
    "telegraf": "^4.16.3",
    "telegraf-ratelimit": "^2.0.0",
    "ws": "^8.18.3",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "prisma": "^7.0.0",
    "jest": "^30.2.0",
    "@jest/environment": "^30.2.0",
    "@jest/globals": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0"
  }
}
